<script id="questions-template" type="x-knockout-template">
    <div class="questionnaire" data-bind="template: { nodes: $component.nodes, data: $component }"></div>
</script>
    <script id="questions-template-two-radio" type="x-knockout-template">
        <div class="row question-row" data-bind="visible: questions[{{index}}].visible">
            <div class="col-sm-8" data-bind="css: { 'child-question': questions[{{index}}].isChild }">
                <span data-bind="html: questions[{{index}}].text"></span>
            </div>
            <div class="col-sm-4">
                <div class="col-sm-12">
                    <div class="panel panel-default question-radio-panel">
                        <div class="panel-body question-radio-body">
                            <div style="position: absolute; right: 20px; top: 5px;">
                                <error-ball params="name: questions[{{index}}].errorName"></error-ball>
                            </div>
                            <div class="col-sm-4">
                                <div class="radio">
                                    <radio-button params="uiFieldName: 'CodeResponse', field: questions[{{index}}].response, enabled: questions[{{index}}].enabled, label: '{{labelOne}}', checkedValue: '{{checkedValueOne}}', groupName: questions[{{index}}].groupName, valueNullable: true"></radio-button>
                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="radio">
                                    <radio-button params="uiFieldName: 'CodeResponse', field: questions[{{index}}].response, enabled: questions[{{index}}].enabled, label: '{{labelTwo}}', checkedValue: '{{checkedValueTwo}}', groupName: questions[{{index}}].groupName, valueNullable: true"></radio-button>
                                </div>
                            </div>
                            <div class="col-sm-4"></div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </script>

