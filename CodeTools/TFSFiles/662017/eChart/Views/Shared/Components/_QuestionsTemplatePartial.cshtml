    <script id="questions-template-checkbox" type="x-knockout-template">
        <div class="row question-row" data-bind="visible: questions[{{index}}].visible">
            <div class="col-sm-12" data-bind="css: { 'child-question': questions[{{index}}].isChild }">
                <div class="col-sm-12">
                    <div >
                        <div >
                            <checkbox params="uiFieldName: 'CodeResponse', field: questions[{{index}}].response, enabled: questions[{{index}}].enabled,customCheckedValue:'{{checkedValue}}',customUncheckedValue:'{{uncheckedValue}}' "></checkbox>
                        </div>
                        <div>
                            <span data-bind="html: questions[{{index}}].text"></span>
                            <error-ball params="name: questions[{{index}}].errorName"></error-ball>
                        </div>
                    </div>
                  </div>
              </div>
        </div>
    </script>

    <script id="questions-template-text" type="x-knockout-template">
