                                </div>
                            </div>
                            <div class="col-sm-4">
                                <div class="radio">
                                    <radio-button params="uiFieldName: 'CodeResponse', field: questions[{{index}}].response, enabled: questions[{{index}}].enabled, label: '{{labelThree}}', checkedValue: '{{checkedValueThree}}', groupName: questions[{{index}}].groupName, valueNullable: true"></radio-button>
